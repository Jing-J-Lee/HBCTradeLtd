<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.xps.hbctradeltd">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:name=".v.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".v.main.MainActivity"
            android:exported="true"
            ></activity>

        <activity
            android:name=".v.LauncherActivity"
            android:theme="@style/hbclauncher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity android:name=".v.login.LoginActivity"
            android:exported="true"
            ></activity>
        <activity android:name=".v.user.UserInfoActivity"
            android:exported="true"
            ></activity>
        <activity android:name=".v.login.ResetPassActivity"
            android:exported="true"
            ></activity>
        <activity android:name=".v.folder.FolderActivity"
            android:exported="true"
            ></activity>
        <activity android:name=".v.editcontract.EditContractActivity"
            android:exported="true"
            ></activity>

        <activity android:name=".v.contact.ContactActivity"
            android:exported="true"
            ></activity>

        <!--auto - layout parmas-->
        <meta-data
            android:name="design_width"
            android:value="1080"></meta-data>

        <meta-data
            android:name="design_height"
            android:value="1920"></meta-data>

    </application>

</manifest>